<% form_for @campaign, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :business_id %><br />
    <%= f.text_field :business_id %>
  </p>
  <p>
    <%= f.label :offer %><br />
    <%= f.text_field :offer %>
  </p>
  <p>
    <%= f.label :shortcode %><br />
    <%= f.text_field :shortcode %>
  </p>
  <p>
    <%= f.label :keyword %><br />
    <%= f.text_field :keyword %>
  </p>
  <p>
    <%= f.label :start_date %><br />
    <%= f.datetime_select :start_date %>
  </p>
  <p>
    <%= f.label :end_date %><br />
    <%= f.datetime_select :end_date %>
  </p>
  <p>
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </p>
  <p>
    <%= f.label :details %><br />
    <%= f.text_area :details %>
  </p>
  <p>
	<%= f.file_field :photo %>
  </p>
  <p><%= f.submit %></p>
<% end %>
